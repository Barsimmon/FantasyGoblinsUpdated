<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>Stake</defName>
    <label>stake</label>
    <description>A spike to place the heads of defeated enemies or friends alike.</description>
    <thingClass>Building_Grave</thingClass>
    <graphicData>
      <texPath>Building/Misc/StakeEmpty</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <constructEffect>ConstructDirt</constructEffect>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <useHitPoints>false</useHitPoints>
    <size>(1,1)</size>
    <tickerType>Rare</tickerType>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <WorkToBuild>800</WorkToBuild>
      <Beauty>-5</Beauty>
    </statBases>
    <rotatable>false</rotatable>
    <costList>
      <WoodLog>5</WoodLog>
    </costList>
    <passability>PassThroughOnly</passability>
    <defaultPlacingRot>North</defaultPlacingRot>
    <building>
      <ai_chillDestination>false</ai_chillDestination>
      <isInert>true</isInert>
      <preventDeteriorationInside>true</preventDeteriorationInside>
      <haulToContainerDuration>500</haulToContainerDuration>
      <fullGraveGraphicData>
        <texPath>Building/Misc/StakeFull/StakeFull</texPath>
        <graphicClass>Graphic_Random</graphicClass>
        <drawSize>(2,2)</drawSize>
      </fullGraveGraphicData>
      <fixedStorageSettings>
        <filter>
          <categories>
            <li>Corpses</li>
          </categories>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <priority>Important</priority>
        <filter>
          <categories>
            <li>CorpsesHumanlike</li>
          </categories>
        </filter>
      </defaultStorageSettings>
    </building>
    <inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
    <terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
    <designationCategory>Misc</designationCategory>
    <designationHotKey>Misc8</designationHotKey>
  </ThingDef>




  <ThingDef ParentName="BuildingBase">
    <defName>Goblin_Banner</defName>
    <label>goblin banner</label>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <description>A goblin banner.</description>
    <graphicData>
      <texPath>Building/Goblin_Banner</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <drawSize>(2,2)</drawSize>
      <color>(231,216,206)</color>
      <shadowData>
        <volume>(0.76,0.3,0.15)</volume>
        <offset>(0,0,-0.39)</offset>
      </shadowData>
    </graphicData>
    <constructEffect>ConstructDirt</constructEffect>
    <startingHpRange>
      <min>0.5</min>
      <max>1.3</max>
    </startingHpRange>
    <statBases>
      <MaxHitPoints>300</MaxHitPoints>
      <WorkToBuild>400</WorkToBuild>

      <Beauty>5</Beauty>
    </statBases>
    <rotatable>false</rotatable>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <fillPercent>0.25</fillPercent>
    <passability>PassThroughOnly</passability>
    <pathCost>100</pathCost>
    <blockWind>true</blockWind>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <costList>
      <WoodLog>15</WoodLog>
      <Cloth>10</Cloth>
    </costList>
    <placeWorkers>
      <li>PlaceWorker_NotUnderRoof</li>
    </placeWorkers>
    <terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
    <designationCategory>Misc</designationCategory>
    <designationHotKey>Misc8</designationHotKey>

  </ThingDef>




  <ThingDef ParentName="FurnitureWithQualityBase">
    <defName>Goblin_WallBanner</defName>
    <label>goblin wall banner</label>
    <thingClass>Building</thingClass>
    <description>A banner made to be hung on walls.</description>
    <category>Building</category>
    <thingCategories>
      <li>Buildings</li>
    </thingCategories>
    <building>
      <isEdifice>false</isEdifice>
    </building>
    <clearBuildingArea>false</clearBuildingArea>
    <graphicData>
      <texPath>Building/Goblin_WallBanner</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawRotated>false</drawRotated>
      <damageData>
        <rect>(0.35,0.35,0.3,0.3)</rect>
      </damageData>
    </graphicData>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <costStuffCount>15</costStuffCount>
    <rotatable>false</rotatable>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>8</pathCost>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <minifiedDef>MinifiedThing</minifiedDef>
    <fillPercent>0.15</fillPercent>
    <statBases>
      <Mass>5</Mass>
      <MaxHitPoints>40</MaxHitPoints>
      <WorkToBuild>160</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>3</Beauty>
    </statBases>
    <selectable>true</selectable>
    <soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
  </ThingDef>


